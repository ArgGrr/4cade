!cpu 6502
!to "build/PRELAUNCH/WB",plain
*=$106

    !source "src/prelaunch/common.a"

         +ENABLE_ACCEL
         lda   #$60
         sta   $426B
         jsr   $4000      ; decompress
         lda   #<callback
         sta   $BD7C
         lda   #>callback
         sta   $BD7D
         +DISABLE_ACCEL
         jmp   $6000

callback
         +ENABLE_ACCEL
         jsr   $BD80      ; decompress
         +DISABLE_ACCEL
         rts

!if * > $1C0 {
  !error "code is too large, ends at ", *
}
